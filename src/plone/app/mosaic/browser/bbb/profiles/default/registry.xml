<?xml version="1.0"?>
<registry>
  <record interface="Products.ResourceRegistries.interfaces.settings.IResourceRegistriesSettings"
          field="resourceBundlesForThemes">
    <!-- p.a.registry does not purge within dictionary field elements and
         that's why all edited bundles must be complete -->
    <value purge="false">
      <element key="Plone Classic Theme">
        <element>jquery</element>
        <element>default</element>
        <element>widgets</element>
        <element>mosaic</element>
      </element>
      <element key="Sunburst Theme">
        <element>jquery</element>
        <element>default</element>
        <element>widgets</element>
        <element>mosaic</element>
      </element>
    </value>
  </record>
</registry>
